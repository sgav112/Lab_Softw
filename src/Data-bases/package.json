{
  "name": "task-manager-backend",
  "version": "1.0.0",
  "description": "Backend for the Task Manager application",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon",
    "build": "tsc",
    "migration:generate": "npx typeorm migration:generate -n",
    "migration:run": "npx typeorm migration:run",
    "migration:revert": "npx typeorm migration:revert",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prettier": "prettier --write 'src/**/*.{js,ts}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.10.3",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
